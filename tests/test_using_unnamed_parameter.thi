min :: (a: i32, b: i32) -> i32 { ret $1 < $2 ? $1 : $2 }
// max :: (a: i32, b: i32) -> i32 { ret $1 > $2 ? $1 : $2 }

main :: () -> i32
{
    ret min(2, 1)
}


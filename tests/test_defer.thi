extern puts (u8*) -> s32

def main () -> s32

    i := 0

    defer 
        puts("3rd")
        i = 1

    defer
        puts("2nd")
        i = 2

    defer
        puts("1st") 
        i = 3

    defer 
        puts("Hello. And welcome.") # <- this is crashing it
        # implicit string literals crash it.
        # we dont handle pointers correctly

    return i


extern puts(u8*) s32


enum Kind
    Int
    Float
    String

main()

    i: Kind = Kind.Float

    if i == .Float
        puts("Its a float!")

    if i 
        is Kind.Int fallthrough
        is 1 puts("Hello")
        is .String puts("are")
        is 3 puts("you?")
        is 4 return 4
        is 5 return 5
        is 6 
            k := 5.0
            return 6
    else 
        return 7

    return 1

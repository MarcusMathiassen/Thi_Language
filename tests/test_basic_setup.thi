extern printf(u8*, ...) s32

def print(msg u8*)
    msg_len = strlen(msg)
    asm
        mov     rax, 0x2000004
        mov     rdi, 1        
        mov     rsi, %msg     
        mov     rdx, %msg_len 
        syscall


def print(msg u8*)
    msg_len = strlen(msg)
    asm 
        mov     rax, 0x2000004  ; write
        mov     rdi, 1          ; stdout
        mov     rsi, %msg       ; msg
        mov     rdx, %msg_len   ; msg length
        syscall
    

load "includes/sys"

def strlen(x u8*) s64 => i = x while *x++ {} x - i
def print(msg u8*) s64
    len = strlen(msg)
    write(STDOUT, msg, len)
    return len

extern malloc(u64) u8*
extern printf(u8*, ...) s32

def main() s32
    buf u8* = malloc(1024)
    read(STDIN, buf, 1024)
    print("Hello\n")
    print(buf)

    printf("%zu\n", mmap(0, 2, 32, -1, 0))
    printf("%d\n",  0x2000000 + 197)

    exit(1)

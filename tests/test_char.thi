extern putchar (s32) -> s32
extern malloc (s64) -> u8*
extern atoi (u8*) -> s32
extern exit (s32) -> u8*
extern system (u8*) -> s32


EOF :: -1

def puts(c: char*) -> s32
    i := 0
    while *(c+sizeof(char)*i)
        if putchar(*(c+sizeof(char)*i)) == EOF
            ret EOF
        i += 1
    if putchar('\n') == EOF
        ret EOF
    ret 1

